    "build:plugin": "cd plugins/export && babel ./ --out-dir ../../",
    "build:plugin:dev": "rm -rf plugins/dist && mkdir plugins/dist && cd plugins/export && babel ./ --out-dir ../dist && cp package.json ../dist/package.json",
